    },
    "license": "proprietary",
    "authors": [
        {
            "name": "cooltronic.pl",
            "email": "craft@cooltronic.pl",
            "homepage": "http://cooltronic.pl",
            "role": "Maintainer"
        },
        {
            "name": "Pawe≈Ç Potacki",
            "homepage": "http://potacki.com",
            "role": "Developer"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "mpdf/mpdf": "^8.0"
